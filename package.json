{
  "name": "vite-vue2-ts-storybook-test-components-library-eslint-prettier-formatonsave",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "npm:clean": "npm cache clean --force && rm -rf node_modules && rm -f package-lock.json",
    "npm:reinstall": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue2": "^2.2.0",
    "pinia": "^2.0.34",
    "sass": "^1.61.0",
    "vite": "^4.2.1",
    "vue": "^2.7.14",
    "vue-tsc": "^1.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vue-eslint-parser": "^9.1.1"
  },
  "peerDependencies": {
    "vue": "^2.7.14"
  }
}
